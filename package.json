{
  "name": "@mattwca/css-module-snapshots",
  "version": "1.1.0",
  "description": "Adds support for testing when using CSS Modules in Jest, including snapshots and style matching.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./transformer": {
      "import": "./dist/transformer/cssModuleTransformer.mjs",
      "require": "./dist/transformer/cssModuleTransformer.cjs"
    }
  },
  "files": [
    "dist",
    "types",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "jest",
    "lint": "eslint src --ext .ts,.js,.cjs"
  },
  "keywords": [
    "jest",
    "css-modules",
    "snapshot",
    "testing",
    "transformer",
    "serializer",
    "matcher"
  ],
  "author": "@mattwca",
  "license": "MIT",
  "dependencies": {
    "@adobe/css-tools": "^4.4.4",
    "cross-spawn": "^7.0.3",
    "postcss": "^8.4.31",
    "postcss-modules": "^6.0.0",
    "sass": "^1.69.5"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattwca/css-module-snapshots.git"
  }
}
